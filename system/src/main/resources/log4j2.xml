<?xml version="1.0" encoding="UTF-8" ?>
<configuration status="debug">


    <Appenders>
        <Console name="consoleAppender" target="SYSTEM_ERR"></Console>
    </Appenders>

    <Loggers>
        <Root level="trace">
            <AppenderRef ref="consoleAppender"></AppenderRef>
        </Root>
    </Loggers>

</configuration>